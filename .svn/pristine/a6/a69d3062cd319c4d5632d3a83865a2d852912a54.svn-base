@keyframes giftanimt
{
    0% {transform:translateY(0);-webkit-transform:translateY(0);-moz-transform:translateY(0);-o-transform:translateY(0);}
    20% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    40% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
    80% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    100% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
}

@-moz-keyframes giftanimt
{
    0% {transform:translateY(0);-webkit-transform:translateY(0);-moz-transform:translateY(0);-o-transform:translateY(0);}
    20% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    40% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
    80% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    100% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
}

@-webkit-keyframes giftanimt
{
    0% {transform:translateY(0);-webkit-transform:translateY(0);-moz-transform:translateY(0);-o-transform:translateY(0);}
    20% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    40% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
    80% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    100% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
}

@-o-keyframes giftanimt
{
    0% {transform:translateY(0);-webkit-transform:translateY(0);-moz-transform:translateY(0);-o-transform:translateY(0);}
    20% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    40% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
    80% {transform:translateY(-5px);-webkit-transform:translateY(-5px);-moz-transform:translateY(-5px);-o-transform:translateY(-5px);}
    100% {transform:translateY(0px);-webkit-transform:translateY(0px);-moz-transform:translateY(0px);-o-transform:translateY(0px);}
}



/*发送礼物显示样式*/
.gift_show{position:relative;width:200px;margin-bottom:1rem;background:rgba(0,0,0,0.5);border-radius:3rem;transition:all 0.6s linear;-webkit-transition:all 0.6s linear;-moz-transition:all 0.6s linear;}
.gift_show .gift_user{width:44px;height:44px;margin-right:5px;vertical-align: middle;border-radius:50%;}
.gift_show .msg_info{display:inline-block;width:130px;white-space:nowrap; overflow:hidden;vertical-align: middle;font-size:14px;color:#fff;}
.gift_show .msg_info a{display:block;width:100%;color:#f1d0ac;white-space:nowrap; overflow:hidden;}
.gift_show .gift_num{position:absolute;left:205px;top:0px;}
.gift_show .gift_img{position:absolute;top:3px;right:0px;width:36px;vertical-align:middle;}
.gift_show .gift_num i{
    display:inline-block;
    color:#fff;
    font-size:30px;
    font-weight:700;
    font-style:normal;
    -webkit-text-stroke: 1px #f6cd94;
    text-stroke: 1px #f6cd94;
    -moz-text-stroke: 1px #f6cd94;
    white-space:nowrap; 
    overflow:hidden; 
    text-overflow:ellipsis;
    text-shadow:0 6px 1px rgba(0,0,0,0.1),
     0 0 5px rgba(0,0,0,0.1),
     0 1px 3px rgba(0,0,0,0.3),
     0 3px 5px rgba(0,0,0,0.2),
     0 5px 10px rgba(0,0,0,0.25);
}

@keyframes animate
{
    0% {transform:scale(1.5);-webkit-transform:scale(1.5);-moz-transform:scale(1.5);-o-transform:scale(1.5);}
    100% {transform:scale(1);-webkit-transform:scale(1);-moz-transform:scale(1);-o-transform:scale(1);}
}

@-moz-keyframes animate
{
    0% {transform:scale(1.5);-webkit-transform:scale(1.5);-moz-transform:scale(1.5);-o-transform:scale(1.5);}
    100% {transform:scale(1);-webkit-transform:scale(1);-moz-transform:scale(1);-o-transform:scale(1);}
}

@-webkit-keyframes animate
{
    0% {transform:scale(1.5);-webkit-transform:scale(1.5);-moz-transform:scale(1.5);-o-transform:scale(1.5);}
    100% {transform:scale(1);-webkit-transform:scale(1);-moz-transform:scale(1);-o-transform:scale(1);}
}

@-o-keyframes animate
{
    0% {transform:scale(1.5);-webkit-transform:scale(1.5);-moz-transform:scale(1.5);-o-transform:scale(1.5);}
    100% {transform:scale(1);-webkit-transform:scale(1);-moz-transform:scale(1);-o-transform:scale(1);}
}

.gift_show .gift_num i{
    animation:animate 0.5s linear 0.4s 1;
    -webkit-animation:animate 0.5s linear 0.4s 1;
    -moz-animation:animate 0.5s linear 0.4s 1;
    -o-animation:animate 0.5s linear 0.4s 1;
}
