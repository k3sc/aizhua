<admintpl file="header" />
</head>
<body>
	<div class="wrap">
		<ul class="nav nav-tabs">
			<li><a href="{:U('LevelLimit/index')}">区间列表</a></li>
			<li class="active"><a >区间添加</a></li>
		</ul>
		<form method="post" class="form-horizontal js-ajax-form" action="{:U('LevelLimit/do_add')}">
			<fieldset>
				
				<div class="control-group">
					<label class="control-label">区间上限</label>
					<div class="controls">
						<input type="text" name="level_up">
						<span class="form-required">*</span>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">提现金额</label>
					<div class="controls">
						<input type="text" name="withdraw">
						<span class="form-required">*</span>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label">序号</label>
					<div class="controls">
						<input type="text" name="orderno" value="0">
						<span class="form-required">*</span>
					</div>
				</div>

			</fieldset>
			<div class="form-actions">
				<button type="submit" class="btn btn-primary js-ajax-submit">{:L('ADD')}</button>
				<a class="btn" href="{:U('LevelLimit/index')}">{:L('BACK')}</a>
			</div>
		</form>
	</div>
	<script src="__PUBLIC__/js/common.js"></script>
</body>
</html>