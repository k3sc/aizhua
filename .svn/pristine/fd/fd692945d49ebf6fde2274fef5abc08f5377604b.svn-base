.topic {
	height: 280px;
	padding: 9px;
	border: 1px solid #eee;
	background-color: #f5f5f5
}

.topic .pic {
	position: relative
}

.topic .pic img {
	display: block;
	width: 100%
}

.topic .pic .mask {
	position: absolute;
	z-index: 3;
	background-color: #000;
	opacity: 0;
	filter: Alpha(opacity=0);
	height: 0;
	width: 100%
}

.topic .pic a:hover .mask {
	top: 0;
	height: 100%;
	opacity: .1;
	filter: Alpha(opacity=10)
}

.topic h3 {
	font-size: 14px;
	text-align: center;
	line-height: 2
}

.topic .desc {
	color: #a4a4a4;
