	color: blue;
	text-shadow: 0 0 3px #fff;
	width: 100%
}

.hjPopGift.hjPopGift_big.active {
	right: 0;
	bottom: -50%;
	opacity: 1;
	height:50px;
	-webkit-transform: scale(1);
	transform: scale(1)
}

.hjPopGift.hjPopGift_big.out {
	opacity: 0;
	right: 300%;
	bottom: -300%;
	height:50px;
	-webkit-transform: scale(0.2);
	transform: scale(0.2)
}

@-webkit-keyframes bigGift {
	0% {
		opacity: 0;
		right: -100%;
		bottom: 100%;
		-webkit-transform: scale(0.2);
		transform: scale(0.2)
	}

	15% {
		opacity: 1;
		right: 0;
		bottom: 0;
		-webkit-transform: scale(1);
		transform: scale(1)
	}

	85% {
		opacity: 1;
		right: 0;
		bottom: 0;
		-webkit-transform: scale(1);
		transform: scale(1)
	}

	92% {
		opacity: 1;
		right: 200%;
		bottom: -1px;
		-webkit-transform: scale(0.1);
		transform: scale(0.1)
	}

	100% {
		opacity: 0;
		right: 300%;
		bottom: -200%;
		-webkit-transform: scale(0.2);
		transform: scale(0.2)
	}
}

@keyframes bigGift {
	0% {
		opacity: 0;
		right: -100%;
		bottom: 100%;
		-webkit-transform: scale(0.2);
		transform: scale(0.2)
	}

	15% {
		opacity: 1;
		right: 0;
		bottom: 0;
		-webkit-transform: scale(1);
		transform: scale(1)
	}

	85% {
		opacity: 1;
		right: 0;
		bottom: 0;
		-webkit-transform: scale(1);
		transform: scale(1)
	}

	92% {
		opacity: 1;
		right: 200%;
		bottom: -1px;
		-webkit-transform: scale(0.1);
		transform: scale(0.1)
	}

	100% {
		opacity: 0;
		right: 300%;
		bottom: -200%;
		-webkit-transform: scale(0.2);
		transform: scale(0.2)
	}
}

@-webkit-keyframes giftNum {
	0% {
		-webkit-transform: scale(4);
		transform: scale(4)
	}

	50% {
		-webkit-transform: scale(1);
		transform: scale(1)
	}

	100% {
		-webkit-transform: scale(1);
		transform: scale(1)
	}
}

@keyframes giftNum {
	0% {
		-webkit-transform: scale(4);
		transform: scale(4)
	}

	50% {
		-webkit-transform: scale(1);
		transform: scale(1)
	}

